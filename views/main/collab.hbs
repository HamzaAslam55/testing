<div class="container">
  <h1 class="text-center">Artist Profiles</h1>

  <div class="row">
    <div class="overview">
      <div class="list-orders-card">
        <div class="table-margin">
          <table class="table">
            <thead>
              <tr>
                <th>Profiles</th>
                
                <th>Status</th>

              </tr>
            </thead>
            <tbody>
              
              {{#each shit}}
              
              <tr>
               
                <td><div ><img class="order-image-my-gig" src="{{photo}} " >
                  <span>{{ name }}</span><br/>
                  <span>{{ email }}</span>
                </div>
                </td>
              
                <td><a href="/users/collab-details/{{_id}}" class="btn btn-primary">Do IT</a></td>
                
              </tr>
              
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>

    </div>
  </div> 

</div>
